NET

scheme testNet = 
extend NET with
class
	value
		net : Net = ["1" +> (2, {}), "2" +>(3, {})],
		netValue: Net = ["1" +> (2, {("1","2",(3,10,2))}), "2" +>(3, {("1","2",(3,10,2))})]
	test_case
		--[empty] empty,
		[insertStop] insertStop("1",2,net),
		[insertStopB] insertStop("2",3,net),
		[isWellformed] isWellformed(netValue),
		[addConnection] addConnection("1","2",2,3,10,net),
		[areDirectlyConnected]areDirectlyConnected("1","2",netValue)
end